#:kivy 1.0

<MainWindow>:
    name: "main"
    GridLayout:
        cols:1
        canvas.before:
            Color:
                rgba:(0/255,51/255,102/255,1)
            Rectangle:
                pos:self.pos
                size:self.size
                
        Button:
            text: "Líder de Desarrollo / Proyect Manager / SDM"
            on_press: 
                self.background_color = (0/255,51/255,102/255,1)
            on_release:
                app.root.current = "perfil1"
                root.manager.transition.direction = "left"
        Button:
            text: "Desarrollador de Software"
            on_press: 
                self.background_color = (0/255,51/255,102/255,1)
            on_release:
                app.root.current = "perfil2"
                root.manager.transition.direction = "left"  
        Button:
            text: "Arquitecto de Software"
            on_press: 
                self.background_color = (0/255,51/255,102/255,1)
            on_release:
                app.root.current = "perfil3"
                root.manager.transition.direction = "left" 

        Button:
            text: "Consultor BI - Business Intelligence"
            on_press: 
                self.background_color = (0/255,51/255,102/255,1)
            on_release:
                app.root.current = "perfil4"
                root.manager.transition.direction = "left" 

        Button:
            text: "Analista de negocios"
            on_press: 
                self.background_color = (0/255,51/255,102/255,1)
            on_release:
                app.root.current = "perfil5"
                root.manager.transition.direction = "left"   

        Button:
            text: "Diseñador Web"
            on_press: 
                self.background_color = (0/255,51/255,102/255,1)
            on_release:
                app.root.current = "perfil6"
                root.manager.transition.direction = "left"        

        Button:
            text: "Analista UX (usabilidad)"
            on_press: 
                self.background_color = (0/255,51/255,102/255,1)
            on_release:
                app.root.current = "perfil7"
                root.manager.transition.direction = "left"      

        Button:
            text: "Tester / Analista tester"
            on_press: 
                self.background_color = (0/255,51/255,102/255,1)
            on_release:
                app.root.current = "perfil8"
                root.manager.transition.direction = "left"                                                                       

        Button:
            text: "Cerrar APP y Enviar Respuestas"
            font_size: (root.width**2 + root.height**2) / 14**4
            on_press: 
                self.background_color = (0/255,51/255,102/255,1)     
            on_release:
                root.fin()

<Perfil1>:
    name: "perfil1"
    pm_actual: pm_actual
    sdm_actual: sdm_actual

    GridLayout:
        cols:1
        canvas.before:
            Color:
                rgba:(0/255,51/255,102/255,1)
            Rectangle:
                pos:self.pos
                size:self.size
        
        FloatLayout:
            size: root.width, root.height/2
            padding: 15
            
            Label:
                text: "Líder de Desarrollo / Proyect Manager / SDM"
                size_hint: 0.8, 0.2
                pos_hint: {"x":0.1, "top":1}
                font_size: (root.width**2 + root.height**2) / 14**4
                
            Label:
                text: "Cantidad Actual en busquedas abiertas"
                size_hint: 0.4,0.1
                pos_hint: {"x":0.45, "top":0.8}
                font_size: (root.width**2 + root.height**2) / 14**4
                            
            Label:
                text: "Gerente de proyectos para empresas tecnologicas: "
                size_hint: 0.4,0.1
                pos_hint: {"x":0.3, "top":0.8-0.13}
                font_size: (root.width**2 + root.height**2) / 15**4      

            Label:
                text: "nueva linea por debajo de gerente de proyectos: "
                size_hint: 0.4,0.1
                pos_hint: {"x":0.3, "top":0.78-0.13}
                font_size: (root.width**2 + root.height**2) / 17**4                            

            TextInput:
                id: pm_actual
                pos_hint: {"x":0.8, "top":0.8-0.13}
                size_hint: 0.1, 0.1
                multiline: False
                font_size: (root.width**2 + root.height**2) / 14**4


            Label:
                text: "Gerente de Operaciones: "
                size_hint: 0.4,0.08
                pos_hint: {"x":0, "top":0.8-0.13*2}                
                font_size: (root.width**2 + root.height**2) / 16**4
                
            TextInput:
                id: sdm_actual
                pos_hint: {"x":0.8, "top":0.8-0.13*2}
                size_hint: 0.1, 0.1   
                multiline: False
                font_size: (root.width**2 + root.height**2) / 14**4
                   

        Button:
            pos_hint:{"x":0.2,"y":0.05}
            size_hint: 0.6, 0.15
            text: "Ingresar valores y volver al menú anterior"
            font_size: (root.width**2 + root.height**2) / 14**4
            on_press: 
                self.background_color = (0/255,51/255,102/255,1)   
            on_release:
                root.manager.transition.direction = "right"            
                root.submit()
                


<Perfil2>:
